<%= @stores.last %> Total Stores <br>

<ul class="stores">
<% @stores[0..-2].each do |store| %>
  <strong><%= store.name %></strong>
  <li> City: <%= store.city %></li>
  <li> Distance: <%= store.distance%> miles</li>
  <li> Phone: <%= store.phone %></li>
  <li> Store Type: <%= store.store_type %></li><br>
<% end %>
</ul>
